CREATE KEYSPACE fakenews WITH REPLICATION = {'class':'SimpleStrategy','replication_factor':'1'}; 
 
use fakenews;


CREATE TABLE users (
            ... user_id          UUID PRIMARY KEY,
            ... first_name     TEXT,
            ... last_name     TEXT,
            ... added_date   TIMESTAMP,
            ... );

DESCRIBE users;

CREATE TABLE combinedData ( index_id          UUID PRIMARY KEY, id     TEXT, news_url     TEXT, title   TEXT, tweet_ids TEXT, Y TEXT, catego
ry TEXT);

index_id,id,news_url,title,tweet_ids,Y,category

COPY combinedData ( index_id          UUID PRIMARY KEY, id     TEXT, news_url     TEXT, title   TEXT, tweet_ids TEXT, Y TEXT, category TEXT) FROM '/home/fake-news-detection/src/data/combined_shufled_data.csv';